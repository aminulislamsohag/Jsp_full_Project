<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">


  <display-name>Login</display-name>
  
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  
  
		<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>servlet.LoginServlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/Login</url-pattern>
		</servlet-mapping>
		
		<servlet>
		<servlet-name>Home</servlet-name>
		<servlet-class>servlet.UserListServlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
		<servlet-name>Home</servlet-name>
		<url-pattern>/UserListServlet</url-pattern>
		</servlet-mapping>
		
		<servlet>
		<servlet-name>SignUP</servlet-name>
		<servlet-class>servlet.SignUpServlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
		<servlet-name>SignUP</servlet-name>
		<url-pattern>/Signup</url-pattern>
		</servlet-mapping>
		
		<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>servlet.LogOutServlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/Logout</url-pattern>
		</servlet-mapping>
		
	
		<servlet>
		<servlet-name>RestPassword</servlet-name>
		<servlet-class>servlet.PasswordResetServlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
		<servlet-name>RestPassword</servlet-name>
		<url-pattern>/PasswordReset</url-pattern>
		</servlet-mapping>
		
	    <servlet>
		<servlet-name>DeleteUser</servlet-name>
		<servlet-class>servlet.UserDeleteServlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
		<servlet-name>DeleteUser</servlet-name>
		<url-pattern>/UserDelete</url-pattern>
		</servlet-mapping>
		
		
		
		
		
		<servlet>
		<servlet-name>add</servlet-name>
		<servlet-class>servlet.InsertSevlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
		<servlet-name>add</servlet-name>
		<url-pattern>/Insert</url-pattern>
		</servlet-mapping>
		
		<servlet>
		<servlet-name>show</servlet-name>
		<servlet-class>servlet.ListSevlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
		<servlet-name>show</servlet-name>
		<url-pattern>/List</url-pattern>
		</servlet-mapping>
		
		<servlet>
		<servlet-name>remove</servlet-name>
		<servlet-class>servlet.DeleteSevlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
		<servlet-name>remove</servlet-name>
		<url-pattern>/Delete</url-pattern>
		</servlet-mapping>
		
		<servlet>
		<servlet-name>update</servlet-name>
		<servlet-class>servlet.UpdateSevlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
		<servlet-name>update</servlet-name>
		<url-pattern>/Edit</url-pattern>
		</servlet-mapping>
  
  
</web-app>